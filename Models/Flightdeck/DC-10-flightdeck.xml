<!-- McDonnell Douglas DC-10 -->
<!-- Flight deck model -->

<PropertyList>

	<path>DC-10-flightdeck.ac</path>

	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>2000</max-m>
	</animation>

	<!-- Pilot-side instruments -->
	<model>
		<path>Aircraft/DC-10-30/Models/Instruments/Clock/clock.xml</path>
		<overlay>
			<params>
				<elapsed-sec>instrumentation/clock[0]/elapsed-sec</elapsed-sec>
				<indicated-hour>instrumentation/clock[0]/indicated-hour</indicated-hour>
				<indicated-min>instrumentation/clock[0]/indicated-min</indicated-min>
				<indicated-timezone>instrumentation/clock[0]/indicated-timezone</indicated-timezone>
				<local-hour>instrumentation/clock[0]/local-hour</local-hour>
				<start-stop-action-script>
				 if (DC10.chronometer1.running)
				  {
				  DC10.chronometer1.stop();
				  }
				 else
				  {
				  DC10.chronometer1.start();
				  }
				</start-stop-action-script>
				<reset-action-script>
				 DC10.chronometer1.reset();
				</reset-action-script>
			</params>
		</overlay>
		<offsets>
			<x-m>-3.063</x-m>
			<y-m>-0.721</y-m>
			<z-m>0.962</z-m>
			<pitch-deg>-10</pitch-deg>
		</offsets>
	</model>
	<model>
		<path>Aircraft/DC-10-30/Models/Instruments/ASI/asi.xml</path>
		<overlay>
			<params>
				<indicated-speed-kt>instrumentation/airspeed-indicator[0]/indicated-speed-kt</indicated-speed-kt>
